<AnchorPane prefHeight="1080.0" prefWidth="1920.0" styleClass="menu_backGround" stylesheets="@mainMenu.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.begindune.MainMenuController">
   <children>
