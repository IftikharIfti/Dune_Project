                  <Font size="16.0" />
               </font></Label>
            <Button fx:id="playButton" alignment="CENTER" mnemonicParsing="false" prefHeight="67.0" prefWidth="126.0" style="-fx-background-radius: 50;" stylesheets="@mainMenu.css" text="Play!">
